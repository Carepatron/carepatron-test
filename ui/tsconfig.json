{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext", "es2015"],
		"sourceMap": true,
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"noEmit": true,
		"jsx": "preserve",
		"isolatedModules": true,
		"noImplicitAny": false,
		"types": ["jest", "node"]
	},
	"include": ["src"]
}
